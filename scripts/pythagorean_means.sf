#!/usr/bin/ruby

# Translation of: http://rosettacode.org/wiki/Averages/Pythagorean_means#Perl

func A(a) {
    a.sum / a.len;
}

func G(a) {
    a.prod.root(a.len);
}

func H(a) {
    a.len / a.map{|n|1/n}.sum;
}

var ints = 1..10;

[
    ["A" => A(ints)],
    ["G" => G(ints)],
    ["H" => H(ints)],
].for {
    "%s=%s\n".printf(_->...)
}
