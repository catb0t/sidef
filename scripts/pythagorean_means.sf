#!/usr/bin/ruby

# Translation of: http://rosettacode.org/wiki/Averages/Pythagorean_means#Perl_6

func A(a) { a.sum / a.len };
func G(a) { a.prod.root(a.len) };
func H(a) { a.len / a.map{1/_}.sum };

static ints = 1..10;
say("A(1,...,10) = ", A(ints));
say("G(1,...,10) = ", G(ints));
say("H(1,...,10) = ", H(ints));
