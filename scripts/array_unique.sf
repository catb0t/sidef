#!/usr/bin/ruby

#
## Removes duplicated items from array
#

var array = [
             1, 2, 3, 'a', 'b', 'c', 2, 3, 4, 'b',
             'c', 'd', 'd', 'd', 'b', '3', '2', 2,
             ["hello"],["hello"],["perl","hacker"]
            ];

array.unique.dump.say;

#
## Same thing, made simple
#

[1,2,1,3,2].unique.dump.say;
