#!/usr/bin/ruby

#
## http://rosettacode.org/wiki/Maximum_triangle_path_sum
#

var sum = [0];
 
ARGF.each {  |line|
    var x = line.words.map{.to_i};
    sum = [
            x.first + sum.first,
            1 .. x.len-2 -> map{|i| x[i] + sum[i-1, i].max}...,
            x.last + sum.last,
        ];
}
 
say sum.max;
