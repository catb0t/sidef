#!/usr/bin/ruby

class Array {
    method insertion_sort {
        { |i|
            var j = i
            var k = self[i]
            while ((j > 0) && (k < self[j - 1])) {
                self[j] = self[j - 1]
                j--
            }
            self[j] = k
        } * self.offset
        return self
    }
}

say (var a = (1..10 -> shuffle))
say a.insertion_sort
