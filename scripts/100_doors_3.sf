#!/usr/bin/ruby

var doors = [];

for (var pass = 1; pass <= 100; pass++) {
    for (var i = 1; i <= 100; i++) {
        i % pass == 0 && (
            doors[i] = (doors[i] := false -> not);
        );
    }
}

for (var i = 1; i <= 100; i++) {
    "Door %d is %s\n".printf(i, (doors[i] ? 'open' : 'closed'));
}
