#!/usr/bin/ruby

var (x₀=1, x₁=7, y₀=3, y₁=4);

var a₀ = (x₀ * y₀);
var b₀ = (a₀ % 10);
assert_eq(3, b₀);

var a₁ = ((x₁ * y₀) + floor(a₀ / 10));
var a₂ = (x₀ * y₁);
var b₁ = ((a₁ % 10) + (a₂ % 10));
assert_eq(5, (b₁ % 10));

var a₃ = ((x₁ * y₁) + floor(a₂ / 10));
var b₂ = (floor(a₁ / 10) + (a₃ % 10) + floor(b₁ / 10));
assert_eq(0, (b₂ % 10));

var b₃ = (floor(a₃ / 10) + floor(b₂ / 10));
assert_eq(3, b₃);

say "** Test passed!";
