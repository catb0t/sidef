#!/usr/bin/ruby

const (
    a = 99,
    b = 42,
)

assert_eq(a, 99)
assert_eq(b, 42)

const *foo = (1,2,3)
const :bar = (a => 14, b => 15)

assert_eq(foo, [1,2,3])
assert_eq(bar, Hash(a => 14, b => 15))

foo[0] = 99
bar{:b} = 42

assert_eq(foo, [99, 2, 3])
assert_eq(bar, Hash(a => 14, b => 42))

bar{:c} = 99

assert_eq(bar, Hash(a => 14, b => 42, c => 99))

say "** Test passed!"
