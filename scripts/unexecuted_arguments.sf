#!/usr/bin/ruby

false
    && ("This should not be printed".say);              # not printed


true
    && false
        && ("Error!".say);                              # not printed

(false || false || true) == true || "Bool error!\n".die;

while (false) {
    "While error!\n".die;
}

true
    && true
        && ("Passed test!".say)                         # printed
            && false
                && ("But failed at the end!".say);      # not printed
